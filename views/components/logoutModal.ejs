<section id="modal">
    <div id="card">
      <div id="head">
        <h2>Logout</h2>

        <% if(admin) { %>
          <a
          id="close"
          href="/admin/<%= user.firstName %><%= user.lastName %>"
          ><i class="material-icons">close</i></a
        >
        <% } else { %>
          <a
          id="close"
          href="/user/<%= user.firstName %><%= user.lastName %>"
          ><i class="material-icons">close</i></a
        >
        <% } %>
  
        
  
  
      </div>
      <hr />
  
      <p>Do you really want to log out?</p>
      <div id="btns">

        <% if(admin) { %>
          <a
          id="btnNot"
          href="/admin/<%= user.firstName %><%= user.lastName %>"
          >No</a
        >
        <% } else { %>
          <a
          id="btnNot"
          href="/user/<%= user.firstName %><%= user.lastName %>"
          >No</a
        >
        <% } %>

        
        <a id="btnYes" href="/user/<%= user.firstName %><%= user.lastName %>/logoutAccount">Yes </a>
      </div>
    </div>
  </section>
  